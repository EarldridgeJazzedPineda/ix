#!/usr/bin/env sh

set -xue

cd ${IX_DIR}

ix_repo_export

cd pkgs/die/scripts/

export LANG=C
export LC_ALL=C

(
cat urls.txt
cat dump.json | ix_flt_upstream
find ${IX_DIR} -name '*.sh' | xargs cat | grep "http[s]*://"
) | grep -v '#'  \
  | grep -v ' '  \
  | grep -v '{{' \
  | grep -v 'skia.google' \
  | ix_urls_flt \
  > _
mv _ urls.txt

ix_fetch | ix_match | sort > _
mv _ bins.json
